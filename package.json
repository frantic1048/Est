    "dev": "webpack --config webpack.dev.config.js",
    "ci": "webpack --config webpack.ci.config.js",
    "lint": "eslint -c .eslintrc.js src test",
    "test": "ava -v --no-power-assert"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/frantic1048/est.git"
  },
  "keywords": [
    "reStructuredText",
